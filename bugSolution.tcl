proc count_lines {filename} {    if {[file exists $filename]} {        set fp [open $filename r]        if {$fp != -1} {          set count 0          while {[gets $fp line] >= 0} {              incr count          }          close $fp          return $count        } else {          return -1; # Or another error code indicating file open failure        }    } else {        return -1; # Or another error code indicating file not found    } }